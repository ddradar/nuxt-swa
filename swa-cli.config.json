{
  "$schema": "https://aka.ms/azure/static-web-apps-cli/schema",
  "configurations": {
    "nuxt-swa": {
      "appDevserverUrl": "http://localhost:3000",
      "apiDevserverUrl": "http://localhost:3000",
      "dataApiLocation": "swa-db-connections",
      "run": "pnpm dev",
      "appLocation": "docs",
      "deploy": {
        "appName": "nuxt-swa",
        "apiLanguage": "node",
        "apiVersion": "20",
        "apiLocation": "docs/.output/server",
        "appLocation": "docs/.output/public",
        "useKeychain": false
      }
    }
  }
}

{
  "name": "nuxt-swa-monorepo",
  "private": true,
  "version": "0.6.0",
  "description": "Nuxt module for Azure Static Web Apps",
  "repository": "ddradar/nuxt-swa",
  "license": "MIT",
  "packageManager": "pnpm@9.9.0+sha512.60c18acd138bff695d339be6ad13f7e936eea6745660d4cc4a776d5247c540d0edee1a563695c183a66eb917ef88f2b4feb1fc25f32a7adcadc7aaf3438e99c1",
  "engines": {
    "pnpm": "9.x"
  },
  "scripts": {
    "prepare": "husky",
    "dev": "swa start",
    "build": "pnpm -C packages/nuxt-swa build && pnpm -C docs build",
    "changelog": "changelogen --bump",
    "format": "pnpm run \"/^format:.*/\"",
    "format:eslint": "eslint . --fix",
    "format:prettier": "pnpm lint:prettier --write",
    "lint": "pnpm run \"/^lint:.*/\"",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier -l \"**/*.{json,yml,js,mjs,ts,vue}\"",
    "test": "pnpm -r test"
  },
  "lint-staged": {
    "*.{js,mjs,ts,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,yml}": "prettier --write"
  },
  "devDependencies": {
    "@azure/static-web-apps-cli": "^2.0.1",
    "@vitest/eslint-plugin": "^1.1.5",
    "changelogen": "^0.5.7",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3"
  }
}

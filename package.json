{
  "name": "@ddradar/nuxt-swa",
  "version": "0.0.1",
  "description": "Nuxt 3 module for Azure Static Web Apps",
  "repository": "ddradar/nuxt-swa",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "husky && nuxi prepare",
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev docs",
    "dev:build": "nuxi build docs",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "format": "pnpm run \"/^format:.*/\"",
    "format:eslint": "pnpm lint:eslint --fix",
    "format:prettier": "pnpm lint:prettier --write",
    "lint": "pnpm run \"/^lint:.*/\"",
    "lint:eslint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore .",
    "lint:prettier": "prettier -l \"**/*.{json,yml,js,ts,vue}\"",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,yml}": "prettier --write"
  },
  "dependencies": {
    "@nuxt/kit": "^3.10.1"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/module-builder": "^0.5.5",
    "@nuxt/schema": "^3.10.1",
    "@nuxt/test-utils": "^3.11.0",
    "@types/node": "18.x",
    "@vitest/coverage-v8": "^1.2.2",
    "@vue/test-utils": "^2.4.4",
    "changelogen": "^0.5.5",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "happy-dom": "^13.3.8",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "nuxt": "^3.10.1",
    "prettier": "^3.2.5",
    "vitest": "^1.2.2"
  }
}

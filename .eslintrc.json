{
  "root": true,
  "extends": ["@nuxt/eslint-config", "prettier"],
  "plugins": ["simple-import-sort"],
  "rules": {
    "@typescript-eslint/no-unused-vars": [
      "warn",
      { "argsIgnorePattern": "^_" }
    ],
    "simple-import-sort/imports": "error",
    "simple-import-sort/exports": "error"
  }
}

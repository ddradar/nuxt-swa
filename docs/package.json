{
  "name": "nuxt-swa-docs",
  "private": true,
  "version": "0.6.0",
  "description": "Nuxt SWA module documentation and playground",
  "type": "module",
  "engines": {
    "node": "18"
  },
  "scripts": {
    "prepare": "pnpm -C ../packages/nuxt-swa prepare:docs && nuxi prepare",
    "dev": "nuxi dev",
    "build": "nuxi build && node -e \"fs.renameSync('./staticwebapp.config.json', './.output/public/staticwebapp.config.json')\"",
    "typecheck": "nuxi typecheck"
  },
  "dependencies": {
    "@iconify-json/heroicons": "^1.2.0",
    "@iconify-json/simple-icons": "^1.2.1",
    "@nuxt/content": "^2.13.2",
    "@nuxt/fonts": "^0.7.2",
    "@nuxt/image": "^1.8.0",
    "@nuxt/ui-pro": "^1.4.1",
    "nuxt": "^3.13.0"
  },
  "devDependencies": {
    "@nuxt/eslint": "^0.5.4",
    "@nuxthq/studio": "^2.0.3",
    "nuxt-applicationinsights": "^0.6.1",
    "nuxt-swa": "workspace:*",
    "vue-tsc": "^2.1.4"
  }
}

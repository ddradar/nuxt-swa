{
  "name": "nuxt-swa-docs",
  "private": true,
  "version": "0.6.0",
  "description": "Nuxt SWA module documentation and playground",
  "type": "module",
  "engines": {
    "node": "18"
  },
  "scripts": {
    "prepare": "pnpm -C ../packages/nuxt-swa prepare:docs && nuxi prepare",
    "dev": "nuxi dev",
    "build": "nuxi build && node -e \"fs.renameSync('./staticwebapp.config.json', './.output/public/staticwebapp.config.json')\"",
    "typecheck": "nuxi typecheck"
  },
  "dependencies": {
    "@iconify-json/heroicons": "^1.2.2",
    "@iconify-json/simple-icons": "^1.2.27",
    "@nuxt/content": "^2.13.4",
    "@nuxt/fonts": "^0.10.3",
    "@nuxt/image": "^1.8.1",
    "@nuxt/ui-pro": "^1.6.0",
    "nuxt": "^3.15.0"
  },
  "devDependencies": {
    "@nuxt/eslint": "^1.0.1",
    "@nuxthq/studio": "^2.2.1",
    "nuxt-applicationinsights": "^0.7.0",
    "nuxt-swa": "workspace:*",
    "vue-tsc": "^2.2.8"
  }
}

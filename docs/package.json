{
  "name": "nuxt-swa-docs",
  "private": true,
  "version": "0.1.0",
  "description": "Nuxt SWA module documentation and playground",
  "type": "module",
  "engines": {
    "node": "18"
  },
  "scripts": {
    "prepare": "pnpm -C ../packages/nuxt-swa prepare:docs && nuxi prepare",
    "dev": "nuxi dev",
    "build": "nuxi build && node -e \"fs.renameSync('./staticwebapp.config.json', './.output/public/staticwebapp.config.json')\"",
    "typecheck": "nuxi typecheck"
  },
  "dependencies": {
    "@iconify-json/heroicons": "^1.1.19",
    "@iconify-json/simple-icons": "^1.1.91",
    "@nuxt/content": "^2.12.0",
    "@nuxt/ui-pro": "^0.7.5",
    "@nuxtjs/fontaine": "^0.4.1",
    "@nuxtjs/google-fonts": "^3.1.3",
    "nuxt": "^3.10.2"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.0.8",
    "@nuxthq/studio": "^1.0.11",
    "nuxt-applicationinsights": "^0.2.1",
    "nuxt-swa": "workspace:*",
    "vue-tsc": "^1.8.27"
  }
}

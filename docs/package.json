{
  "name": "nuxt-swa-docs",
  "private": true,
  "version": "0.5.0",
  "description": "Nuxt SWA module documentation and playground",
  "type": "module",
  "engines": {
    "node": "18"
  },
  "scripts": {
    "prepare": "pnpm -C ../packages/nuxt-swa prepare:docs && nuxi prepare",
    "dev": "nuxi dev",
    "build": "nuxi build && node -e \"fs.renameSync('./staticwebapp.config.json', './.output/public/staticwebapp.config.json')\"",
    "typecheck": "nuxi typecheck"
  },
  "dependencies": {
    "@iconify-json/heroicons": "^1.1.23",
    "@iconify-json/simple-icons": "^1.1.110",
    "@nuxt/content": "^2.12.1",
    "@nuxt/fonts": "^0.7.0",
    "@nuxt/ui-pro": "^1.2.0",
    "nuxt": "^3.11.2"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.2.0",
    "@nuxt/eslint": "^0.3.10",
    "@nuxthq/studio": "^2.0.3",
    "nuxt-applicationinsights": "^0.6.0",
    "nuxt-swa": "workspace:*",
    "vue-tsc": "^2.0.29"
  }
}
